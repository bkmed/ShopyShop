{
  "name": "shopyshop",
  "version": "0.0.1",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build-web-dev": "npm-run-all --parallel webpack-web-dev dev:build-web-server",
    "build-web-prod": "npm-run-all --parallel webpack-web-prod prod:build-web-server",
    "start-web-dev": "npm-run-all --parallel webpack-web-dev dev:*",
    "start-web-prod": "npm-run-all --parallel webpack-web-prod prod:*",
    "webpack-web-dev": "webpack --mode development",
    "dev:build-web-server": "NODE_ENV=development webpack --config webpack.server.js --mode=development -w",
    "dev:start-web": "nodemon ./server-build/index.js",
    "webpack-web-prod": "NODE_ENV=production webpack --mode=production",
    "prod:build-web-server": "NODE_ENV=production webpack --config webpack.server.js --mode=production -w",
    "prod:start-web": "node ./server-build/index.js",
    "build-gh-pages": "npm run webpack-web-prod",
    "start-web-dev-nossr": "webpack-dev-server --mode development --open --hot",
    "start-android": "react-native run-android && adb reverse tcp:12030 tcp:12030",
    "start-ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "verify-translations": "node scripts/verify_translations.js",
    "postinstall": "patch-package",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "detox:build": "detox build",
    "detox:test": "detox test",
    "format": "bash scripts/format.sh",
    "rename-project": "node scripts/rename-project.js"
  },
